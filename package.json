{
  "name": "wac-skripta",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Ionic App Starter",
  "scripts": {
    "build": "stencil build",
    "start": "concurrently --kill-others \"npm run makehtml:watch\" \"stencil build --dev --watch --serve\" \"node ./dev-utils/dev-proxy.js\"",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "makehtml": "node ./build/makehtml/makehtml.js",
    "makehtml:watch": "node ./build/makehtml/makehtml.js --watch",
    "dev-proxy": "node ./dev-utils/dev-proxy.js"
  },
  "devDependencies": {
    "@stencil/core": "3.2.0",
    "@types/jest": "^29.5.0",
    "concurrently": "^8.1.0",
    "gaze": "^1.1.3",
    "http-proxy": "^1.18.1",
    "ionicons": "^7.1.0",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "puppeteer": "^19.8.0",
    "showdown": "^2.1.0",
    "workbox-build": "^4.3.1"
  },
  "license": "MIT",
  "dependencies": {
    "@material/web": "^1.0.0-pre.8",
    "highlight.js": "^11.8.0",
    "html-entities": "^2.3.3",
    "stencil-router-v2": "^0.6.0"
  }
}
